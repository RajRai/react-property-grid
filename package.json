{
  "name": "@rajrai/react-property-grid",
  "version": "0.1.0",
  "description": "Condensed hierarchical property grid for React + MUI",
  "author": "Raj Rai <raj.rai.ct@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rajrai/react-property-grid.git"
  },
  "bugs": {
    "url": "https://github.com/rajrai/react-property-grid"
  },
  "homepage": "https://github.com/rajrai/react-property-grid#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "keywords": [
    "react",
    "mui",
    "material-ui",
    "property-grid",
    "editor"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && vite build && npm run copy-meta",
    "clean": "rimraf dist",
    "copy-meta": "copyfiles LICENSE README.md package.json dist",
    "play": "vite --config playground/vite.config.js"
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^5.15.0 || ^6.0.0",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1",
    "vite": "^5.4.0"
  }
}
